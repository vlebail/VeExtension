{
  "manifest_version": 2,
  "name":"VeExtension",
  "version":"1.0.1",
  "description":"Extension VeInteractive. Navigue plus facilement dans l'ERP",
  "browser_action":
  {
    "default_icon":"./logo.png",
    "default_popup":"main.html"
  },
  "background": {
    "page": "main.html",
    "persistent":false
  },
  "permissions": [
    "tabs",
    "http://*/*", "https://*/*", "background"
  ]
}
